<h1>New Restaurant</h1>
  
<%= form_for (@restaurant) do |f| %>
   <h3>Name</h3>
  <%= f.text_field :name %>
  <h3>Address</h3>
  <%= f.text_field :address %>
  <h3>Phone number</h3>
  <%=  f.text_field :phone_number %>
  <h3>Category</h3>
  <%= f.text_field :category %>
  <%= f.submit %>
<% end %>
<%= link_to 'Back', restaurants_path %>